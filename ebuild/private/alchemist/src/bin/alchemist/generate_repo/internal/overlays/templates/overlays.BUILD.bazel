load("@//bazel/ebuild/private:overlay.bzl", "overlay_set")

{%- for overlay_set in overlay_sets %}

overlay_set(
    name = "{{ overlay_set.name }}",
    overlays = [
        {%- for overlay in overlay_set.overlays %}
        "{{ overlay }}",
        {%- endfor %}
    ],
    visibility = ["@//bazel:internal"],
)
{%- endfor %}
